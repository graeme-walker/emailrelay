	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(e_docdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(e_docdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(e_docdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(e_docdir)/$$p; \
	  fi; fi; \
	done

